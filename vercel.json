{
    "version": 2,
    "env": {
        "MYSQL_HOST": "localhost"
    },
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "post/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/user(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/post(.*)",
        "dest": "/post/index.js"
      }
    ]
  }